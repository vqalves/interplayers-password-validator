{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PasswordValidationOptions": {
    "MinimumCharacterCount": 9,
    "MinimumAmountOfNumbers": 1,
    "MinimumAmountOfLowerCaseLetters": 1,
    "MinimumAmountOfUpperCaseLetters": 1,
    "MaximumAmountOfRepeatedCharacters": 1,
    "MinimumAmountOfSpecialCharacters": 1,
    "SpecialCharacters": ["!", "@", "#", "$", "%", "^", "&", "*", "(", ")", "-", "+"]
  },
  "AllowedHosts": "*"
}
